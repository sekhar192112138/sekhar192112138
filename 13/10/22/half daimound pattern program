#include <stdio.h>  
  
int main()  
{  
    int n,m=1,a;  
    printf("Enter the number of columns");  
    scanf("%d",&n);  
    printf("the number you want to print= ");
    scanf("%d",&a);
for(int i=1;i<=n;i++)  
{  
  for(int j=1;j<=i;j++)  
  {  
    printf("%d",a);  
  }  
  printf("\n");  
}  
 for(int i=n-1;i>=1;i--)  
 {  
   for(int j=1;j<=i;j++)  
   {  
     printf("%d",a);  
   }  
   printf("\n");  
 }    
     
    return 0;  
}  
