#include <stdio.h>
#include <stdlib.h>
int main()
{
 int fno,sno,*ptr1=&fno,*ptr2=&sno; 
   printf("\n\n Pointer : Find the maximum number between two numbers :\n"); 
   printf("\n");   
   printf(" Input the first number : ");
   scanf("%d", ptr1);
   printf(" Input the second  number : ");
   scanf("%d", ptr2); 
 if(*ptr1>*ptr2)
 {
  printf("\n\n %d is the maximum number.\n\n",*ptr1);
 }
 else
 {
  printf("\n\n %d is the maximum number.\n\n",*ptr2);
 }

}
 
     TEST CASE 1
     Pointer : Find the maximum number between two numbers :
     Input the first number : 0
     Input the second  number : 4
     4 is the maximum number.
    
    
     TEST CASE 2
     Pointer : Find the maximum number between two numbers :
     Input the first number : 5
     Input the second  number : 0
     5 is the maximum number.
     
     
     TEST CASE 3
     Pointer : Find the maximum number between two numbers :
     Input the first number : -3
     Input the second  number : 3
     3 is the maximum number.
     
     
     TEST CASE 4
     Pointer : Find the maximum number between two numbers :
     Input the first number : 0
     Input the second  number : 0
     0 is the maximum number.
     
     
     TEST CASE 5
     Pointer : Find the maximum number between two numbers :
     Input the first number : 123
     Input the second  number : 123
     123 is the maximum number.
